		this.element = element;
		ArrayList<Edge<T>> edges= new ArrayList<Edge<T>>();
	}
	
	public T getElement(){
		return element;
	}
	
	
	public void addEdge(){
		
	}
		
	public Arraylist<Edge<T>> getEdges(){
		return edges;
	}
}
